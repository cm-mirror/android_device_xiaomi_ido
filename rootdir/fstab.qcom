# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#TODO: Add 'check' as fs_mgr_flags with data partition.
# Currently we dont have e2fsck compiled. So fs check would failed.

#<src>                                          <mnt_point>		<type>  <mnt_flags and options>			<fs_mgr_flags>
/dev/block/bootdevice/by-name/system		/system                 ext4    ro                                      wait
/dev/block/bootdevice/by-name/userdata		/data                   ext4    nosuid,nodev,noauto_da_alloc            wait,check,formattable,resize,encryptable=footer
/dev/block/bootdevice/by-name/cache             /cache                  ext4	nosuid,nodev                            wait
/dev/block/bootdevice/by-name/persist		/persist                ext4	nosuid,nodev                            wait
/dev/block/bootdevice/by-name/cust              /cust                   ext4    nosuid,nodev                            wait,check
/dev/block/bootdevice/by-name/boot              /boot                   emmc	defaults                                defaults
/dev/block/bootdevice/by-name/recovery		/recovery               emmc	defaults                                defaults 
/dev/block/bootdevice/by-name/config		/frp                    emmc    defaults                                defaults
/dev/block/bootdevice/by-name/modem		/firmware                    vfat    ro,context=u:object_r:firmware_file:s0,shortname=lower,uid=1000,gid=1000,dmask=227,fmask=337                                wait

# sd
/devices/soc.0/7864900.sdhci/mmc_host*		auto			auto    nosuid,nodev                            wait,voldmanaged=sdcard1:auto,noemulatedsd
/devices/platform/msm_hsusb*			auto			auto    nosuid,nodev                            wait,voldmanaged=usbotg:auto
/dev/block/zram0				none                    swap    defaults                                zramsize=536870912
